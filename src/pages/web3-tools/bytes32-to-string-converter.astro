---
import Bytes32ToStringConverter from "../../views/Bytes32ToStringConverter.astro";
import BaseLayout from "../../layouts/BaseLayout.astro";
import Seo from "../../components/Seo.astro";

import styles from "./bytes32-to-string-converter.scss?inline";
---

<BaseLayout>
  <Seo
    slot="seo"
    title="Online Solidity Bytes32 to String Converter"
    description="Efficiently convert solidity's bytes32 data type to string. Additionally, discover other useful web3 tools."
    og={{
      image: "/assets/images/meta/og/web3-bytes32-to-string.png",
      imageAlt: "Bytes32 to String Converter",
    }}
  />
  <style slot="header" is:inline set:html={styles}></style>
  <Bytes32ToStringConverter />

  <Fragment slot="footer">
    <script src="../../scripts/react/index.js"></script>
  </Fragment>
</BaseLayout>
